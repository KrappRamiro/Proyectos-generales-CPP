};

int main() {
	struct dispositivos datos[cantidadDispositivos];

	int i=5;
	int resu=0;
	prueba(&i); //aca paso la direccion de memoria de i
	cout << i <<endl;
}

void ingresarDatos(struct dispositivos * datos) { //tipo de dato struct dispositivo, que me pase un puntero que yo llame datos

}

void prueba(int* a) {
	*a++;
	return;
}
